#include <vector>
#include "Member.h"
#include "Motorbike.h"

using namespace std;

class Request
{
private:
    int requestID;
    vector<Member *> userinfo;
    vector<Motorbike *> bikeinfo;

public:
    Request(int ID = 0, vector<Member *> user = {}, vector<Motorbike *> bike = {})
    {
        this-> requestID = ID;
        this-> userinfo = user;
        this-> bikeinfo = bike;
    }

    
};
